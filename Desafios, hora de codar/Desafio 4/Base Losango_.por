programa {
  funcao inicio() {

   real diagonal_maior, diagonal_menor, area

  escreva ("Informe a diagonal maior do Losango:")
  leia(diagonal_maior)

  escreva ("Informe a diagonal menor do Losango:")
  leia(diagonal_menor)

  area = diagonal_maior * diagonal_menor /2

  escreva("A área do Losango é: ", area)
  }
}
