programa {
  funcao inicio() {
    
  
   // Declaração das variáveis
    real lado, area

    // Entrada de dados
    escreva("Informe o lado do quadrado: ")
    leia(lado)
    
    // Cálculo da área
    
    area = lado * lado

    // Exibição do resultado
    escreva("A área do retângulo é: ", area)
    
  }
}
 